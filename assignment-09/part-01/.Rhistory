v <- c("1","1")
m[7,] <- vF4Prod("1",v)
m[8,] <- vF4Prod("x",v)
m[9,] <- vF4Prod("x+1",v)
v <- c("1","x")
m[10,] <- vF4Prod("1",v)
m[11,] <- vF4Prod("x",v)
m[12,] <- vF4Prod("x+1",v)
v <- c("1","x+1")
